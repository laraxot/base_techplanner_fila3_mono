{
    "name": "laraxot/module_chart_fila3",
    "description": "chart module that manages your data and represents them in graphs",
    "keywords": [
        "laraxot",
        "laravel",
        "filament",
        "module_chart",
        "Laravel",
        "Chart module",
        "Charts"
    ],
    "homepage": "https://github.com/laraxot/module_chart_fila3",
    "license": "MIT",
    "authors": [{
        "name": "Marco Sottana",
        "email": "marco.sottana@gmail.com"
    }],
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Chart\\Providers\\ChartServiceProvider",
                "Modules\\Chart\\Providers\\Filament\\AdminPanelProvider"
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Chart\\": "app/",
            "Modules\\Chart\\Database\\Factories\\": "database/factories/",
            "Modules\\Chart\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "require": {
        "amenadiel/jpgraph": "^4.1"
    },
    "repositories": [ {
        "type": "path",
        "url": "../Xot"
    },
    {
        "type": "path",
        "url": "../Tenant"
    },
    {
        "type": "path",
        "url": "../UI"
    }],
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/pest --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

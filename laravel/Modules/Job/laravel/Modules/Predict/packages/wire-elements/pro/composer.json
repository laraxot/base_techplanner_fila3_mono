{
    "name": "wire-elements/pro",
    "description": "Beautiful components crafted with Livewire",
    "keywords": [
        "wire-elements",
        "laravel",
        "components",
        "livewire"
    ],
    "homepage": "https://github.com/wire-elements/pro",
    "license": "proprietary",
    "authors": [
        {
            "name": "Philo Hermans",
            "email": "support@wire-elements.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "livewire/livewire": "*",
        "spatie/laravel-package-tools": "*",
        "illuminate/contracts": "*"
    },
    "require-dev": {
        "laravel/pint": "*",
        "nunomaduro/collision": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "WireElements\\Pro\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WireElements\\Pro\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WireElements\\Pro\\WireElementsProServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

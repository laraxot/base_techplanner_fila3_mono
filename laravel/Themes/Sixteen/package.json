{
    "name": "sixteen",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:analyze": "vite build --mode analyze",
        "build:production": "vite build --mode production",
        "preview": "vite preview",
        "copy": "cp -r ./public/* ../../../public_html/themes/Sixteen/",
        "copy:watch": "nodemon --watch ./public --exec 'npm run copy'",
        "lint": "eslint resources/js/**/*.js",
        "lint:fix": "eslint resources/js/**/*.js --fix",
        "format": "prettier --write resources/**/*.{js,css,blade.php}",
        "test": "jest",
        "test:watch": "jest --watch",
        "analyze": "vite build --mode analyze",
        "analyze:serve": "npx serve dist",
        "bundle-report": "npm run analyze && open dist/stats.html"
    },
    "devDependencies": {
        "@alpinejs/focus": "^3.14.1",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.16",
        "autoprefixer": "^10.4.21",
        "axios": "^1.7.4",
        "concurrently": "^9.0.1",
        "daisyui": "^4.12.22",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "flowbite": "^2.5.1",
        "jest": "^29.7.0",
        "laravel-vite-plugin": "^1.3.0",
        "nodemon": "^3.0.3",
        "postcss": "^8.5.6",
        "postcss-import": "^16.1.1",
        "postcss-nesting": "^13.0.2",
        "prettier": "^3.6.2",
        "rollup-plugin-visualizer": "^5.12.0",
        "swiper": "^11.1.10",
        "tailwindcss": "^3.4.17",
        "terser": "^5.28.1",
        "vanilla-cookieconsent": "^3.0.1",
        "vite": "^6.3.5",
        "vite-plugin-compression": "^0.5.1"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.8",
        "bootstrap-italia": "^2.16.0"
    }
}

{
    "name": "laraxot/theme_sixteen_fila3",
    "description": "Sixteen theme for Laraxot projects (AGID-compliant, Tailwind-based)",
    "keywords": [
        "laraxot",
        "laravel",
        "filament",
        "theme_sixteen",
        "Laravel",
        "Ticket module"
    ],
    "homepage": "https://github.com/laraxot/theme_sixteen_fila3",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Xot",
            "email": "marco.sottana@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {},
    "extra": {
        "laravel": {
            "providers": [
                "Themes\\Sixteen\\Providers\\ThemeServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require": {
        "filament/filament": "^3.4",
        "filament/tables": "^3.4",
        "filament/forms": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Themes\\Sixteen\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^9.4"
    }
}
